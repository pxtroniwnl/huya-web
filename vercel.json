{
  "builds": [
    {
      "src": "backend/calculate_weather.php",
      "use": "@vercel/php"
    },
    {
      "src": "backend/python.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "/backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
